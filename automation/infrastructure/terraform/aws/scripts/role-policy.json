{
  "Version":"2012-10-17",
  "Statement":[
     {
        "Effect": "Allow",
        "Action": [
           "s3:GetBucketLocation",
           "s3:GetObject",
           "s3:ListBucket" 
        ],
        "Resource": [
           "arn:aws:s3:::sherlock-raw-images",
           "arn:aws:s3:::sherlock-raw-images/*"
        ]
     },
     {
        "Effect": "Allow",
        "Action": [
           "s3:GetBucketLocation",
           "s3:GetObject",
           "s3:ListBucket",
           "s3:PutObject",
           "s3:GetBucketAcl"
        ],
        "Resource": [
           "arn:aws:s3:::sherlock-raw-images",
           "arn:aws:s3:::sherlock-raw-images/*"
           "arn:aws:s3:::sherlock-raw-sd-image",
           "arn:aws:s3:::sherlock-raw-sd-image/*"
        ]
     },
     {
        "Effect": "Allow",
        "Action": [
           "ec2:ModifySnapshotAttribute",
           "ec2:CopySnapshot",
           "ec2:RegisterImage",
           "ec2:Describe*"
        ],
        "Resource": "*"
     }
  ]
}